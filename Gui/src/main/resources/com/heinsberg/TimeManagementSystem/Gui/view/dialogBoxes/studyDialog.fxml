      <ButtonType fx:constant="OK" />
      <ButtonType fx:constant="CLOSE" />
   </buttonTypes>
   <graphic>
      <StackPane prefWidth="500.0">
         <children>
            <GridPane prefHeight="117.0" prefWidth="420.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <StackPane.margin>
                  <Insets left="40.0" right="40.0" />
               </StackPane.margin>
               <children>
                  <Label text="Studiums Name:" />
                  <TextField fx:id="studyNameField" GridPane.columnIndex="1" />
               </children>
            </GridPane>
         </children>
      </StackPane>
